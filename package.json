{
  "name": "webnizer",
  "displayName": "Webnizer",
  "description": "Webnizer VS Code Extension",
  "version": "0.0.1",
  "publisher": "intc-wpe",
  "engines": {
    "vscode": "^1.67.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:webnizer.ui",
    "onCommand:webnizer.build"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "webnizer.ui",
        "title": "Webnizer - Hello"
      },
      {
        "command": "webnizer.build",
        "title": "Webnizer - Build your project",
        "icon": {
          "light": "./src/media/favicon.png",
          "dark": "./src/media/favicon.png"
        }
      }
    ],
    "keybindings": [
      {
        "command": "webnizer.build",
        "key": "ctrl+f1",
        "mac": "cmd+f1",
        "when": "editorTextFocus"
      }
    ],
    "menus": {
      "editor/title": [
        {
          "when": "resourceLangId == typescript || cpp || c || rust",
          "command": "webnizer.build",
          "group": "navigation"
        }
      ],
      "editor/context": [
        {
          "when": "resourceLangId == typescript || cpp || c || rust",
          "command": "webnizer.build",
          "group": "navigation"
        }
      ],
      "commandPalette": [
        {
          "when": "resourceLangId == typescript || cpp || c || rust",
          "command": "webnizer.build",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.67.0",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "14.x",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "eslint": "^8.14.0",
    "glob": "^8.0.1",
    "mocha": "^10.0.0",
    "typescript": "^4.6.4",
    "@vscode/test-electron": "^2.1.3"
  }
}
